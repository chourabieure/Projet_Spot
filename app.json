{
  "expo": {
    "name": "Spot",
    "slug": "app_spot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_lunch.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F5FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_lunch.png",
        "backgroundColor": "#F7F5FF"
      },
      "package": "app.spot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}